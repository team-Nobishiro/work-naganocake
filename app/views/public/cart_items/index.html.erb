<div class="container">
    <div class="row">
    </div>
    <h2>ショッピングカート</h2>


<table>
<thead>
     <tr>
      <th>商品名</th>
      <th>単価(税込)</th>
       <th>数量</th>
       <th>小計</th>
       </tr>
        </thead>
        <body>
          <% @current_end_user.cart_items.each do |cart_item| %>
          <tr> 
          <td>
           <%= cart_item.item.name %>
         </td>

         <td>
           <%= cart_item.item_id %>
         </td>
         <td>
           <%= cart_item.number %>
         </td>
         </tr>

          <% end %>
       
         

        </body>
</table>
</div>
