<div class="container main">
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
      <h2>会員編集画面</h2>
      <%= form_for (@end_user), url: admin_end_user_path(@end_user) do |f| %>
      <div class="form-group">
        <%= f.label :会員id, class: "edit-u" %>
        <%= f.label @end_user.id, class: "" %>
      </div>
      <div class="form-group">
        <%= f.label :氏名, class: "edit-u" %>
        <%= f.text_field :last_name, class: "edit-user" %>
        <%= f.text_field :first_name, class: "edit-user" %>
      </div>
      <div class="form-group">
        <%= f.label :フリガナ, class: "edit-u" %>
        <%= f.text_field :last_name_kana, class: "edit-user" %>
        <%= f.text_field :first_name_kana, class: "edit-user" %>
      </div>
      <div class="form-group">
        <%= f.label :郵便番号, class: "edit-u" %>
        <%= f.text_field :postal_code, class: "edit-user" %>
      </div>
      <div class="form-group">
        <%= f.label :住所, class: "edit-u" %>
        <%= f.text_field :address, class: "edit-user" %>
      </div>
      <div class="form-group">
        <%= f.label :電話番号, class: "edit-u" %>
        <%= f.text_field :phone_number, class: "edit-user" %>
      </div>
      <div class="form-group">
        <%= f.label :メールアドレス, class: "edit-u" %>
        <%= f.email_field :email, class: "edit-user" %>
      </div>
      <div class="">
        <%= f.label :会員ステータス, class: "edit-u" %>
        <div class="form-check">
            <%= f.radio_button :is_withdrawal, :true %>
            <%= f.label :is_withdrawal, "有効会員" %>
            <%= f.radio_button :is_withdrawal, :false %>
            <%= f.label :is_withdrawal, "退会済み" %>
        </div>
      </div>
      <div class="">
        <%= f.submit "変更を保存する", class: "btn btn-primary" %>
      </div>
      <% end %>
    </div>
  </div>
</div>

  
