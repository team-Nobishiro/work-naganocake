<table class='table'>
  <thead>
    <tr>
      <th>氏名</th>
      <th>フリガナ</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% end_users.each do |user| %>
      <tr>
        <td><%= user.last_name %><%= user.first_name %></td>
        <td><%= user.last_name_kana %><%= user.first_name_kana %></td>
        <td><%= link_to '詳細', admin_end_user_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
