<div class="container">
  <div class="row">
    <h3>会員情報一覧</h3>
  </div>
         

    
    <table class="table">
    <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
        </tr> 
      </thead>

      <tbody>
      <% @end_users.each do |end_user| %>
      <tr>
        <td><%= end_user.id %></td>
        
        <td><%= link_to end_user.last_name, admin_end_user_path(end_user) %></td>
        
      </td>
      <td><%= end_user.email %></td>
      <td><% if end_user.is_withdrawal == true %>
            退会済み
          <% else %>
            有効会員
          <% end %>
      </td>
      </tr>
      <% end %>
      </tbody>
      </table>
    </div>
  </div>
</div>




     
