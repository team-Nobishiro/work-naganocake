<div class="container main">
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
      <h3>会員情報一覧</h3>
      <table class="user-info">
        <thead>
          <tr>
            <th class="user-t">会員ID</th>
            <th class="user-t">氏名</th>
            <th class="user-t">メールアドレス</th>
            <th class="user-t">ステータス</th>
          </tr> 
        </thead>
        <tbody>
          <% @end_users.each do |end_user| %>
          <tr>
            <td><%= end_user.id %></td>
            <td><%= link_to end_user.last_name, admin_end_user_path(end_user), :style => "color: #C7B299;" %></td>
          </td>
          <td><%= end_user.email %></td>
          <td>
            <% if end_user.is_withdrawal == true %>
              退会済み
            <% else %>
              有効会員
            <% end %>
          </td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>




     
